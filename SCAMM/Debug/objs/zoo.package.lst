                                      8 ; Call Method (object in IY)
                                      9 ;---------------------------------------------------------
                                     10 ; Call method for object IY, considering polymorphism.
                                     11 ; The class parameter is the base class for polymorphism.
                                     12 ; If a direct call to the method of a specific class is intended,
                                     13 ; please use "call <class>._<method>" directly.
                                     14 ; IY, HL and BC cannot be used to transfer parameters.
                                     15 ;---------------------------------------------------------
                                     16 ; Input:
                                     17 ;	IY			= pObject
                                     18 ; Output:
                                     19 ;---------------------------------------------------------
                                     20 	.macro Zoo.CallMethod_IY class, method
                                     21 	Zoo.GetClass_IY H, L
                                     22 	ld bc, #class'._'method'.offset
                                     23 	add hl, bc
                                     24 	ld c, (hl)
                                     25 	inc hl
                                     26 	ld h, (hl)
                                     27 	ld l,c
                                     28 	call _zoo_call_hl
                                     29 	.endm
                                     30 
